{
    "name": "PxTrax",
    "description": "funky things I like to have on Amazon.com!",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/pxtrax16.png",
            "32": "/images/pxtrax32.png",
            "48": "/images/pxtrax48.png",
            "128": "/images/pxtrax128.png"
        }
    },
    "icons": {
        "16": "/images/pxtrax16.png",
        "32": "/images/pxtrax32.png",
        "48": "/images/pxtrax48.png",
        "128": "/images/pxtrax128.png"
    }
}
